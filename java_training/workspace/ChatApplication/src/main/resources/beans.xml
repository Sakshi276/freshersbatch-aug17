<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-4.2.xsd">


<bean id="chatroom1" class="com.spring.ChatApplication.ChatRoomBean">
	 
	 <property name="users">          
		 <set>              
			 <value>Ivan</value>              
			 <value>Tom</value>              
	 		             
			 <value>Rahul</value>          
	 	</set>      
	 </property>
	 
	  <property name="messages">          
	  	<list>              
	  		<value>Hello</value>              
	  		<value>hey</value>              
	  		<value>hii</value>              
	  		<value>Good!</value>          
	  	</list>      
	  </property>  

</bean>

<bean id="chatroom2" class="com.spring.ChatApplication.ChatRoomBean">
	 
	 <property name="users">          
		 <set>              
			 <value>Ivan2</value>              
			 <value>Tom2</value>              
	 		             
			 <value>Rahul2</value>          
	 	</set>      
	 </property>
	 
	  <property name="messages">          
	  	<list>              
	  		<value>Hello2</value>              
	  		<value>hey2</value>              
	  		<value>hii</value>              
	  		<value>Good!</value>          
	  	</list>      
	  </property>  

</bean>



</beans>